# Пример файла .env для EduLife_Dock
# Скопируйте этот файл в .env и настройте параметры

# Базовые настройки
DATABASE_URL=sqlite:///./document_service.db
APP_SECRET_KEY=your-secure-secret-key-for-jwt
DEBUG=False
LOG_LEVEL=INFO

# Настройки интеграции с внешними сервисами
# URL сервиса аутентификации
AUTH_SERVICE_URL=http://localhost:8070
# URL сервиса расписания
RASPIS_SERVICE_URL=http://localhost:8090
# URL QR-сервиса
QR_SERVICE_URL=http://localhost:8080

# Настройки безопасности
ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_ALGORITHM=HS256

# Настройки порта (если не используется uvicorn с явным указанием порта)
PORT=8100